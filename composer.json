{
  "name": "gulaandrij/speaker-recognition-bundle",
  "type": "symfony-bundle",
  "description": "Integrates Speaker Recognition API from Microsoft Cognitive Services with Symfony",
  "keywords": [
    "symfony2",
    "speech",
    "speaker",
    "recognition",
    "verify",
    "identify",
    "enroll",
    "voice"
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "ext-openssl": "*",
    "guzzlehttp/guzzle": "^6.3.3",
    "symfony/config": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/http-foundation": "^4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13",
    "liip/rmt": "^1.4"
  },
  "suggest": {
    "vich/uploader-bundle": "To add audio file as entity field"
  },
  "extra": {
    "thanks": {
      "name": "onekit/speaker-recognition-bundle",
      "url": "https://github.com/onekit/speaker-recognition-bundle"
    }
  },
  "autoload": {
    "psr-4": {
      "Lavulator\\SpeakerRecognitionBundle\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
