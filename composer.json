{
  "name": "gulaandrij/speaker-recognition-bundle",
  "type": "symfony-bundle",
  "description": "Integrates Speaker Recognition API from Microsoft Cognitive Services with Symfony2.",
  "keywords": ["symfony2", "speech", "speaker", "recognition", "verify", "identify", "enroll", "voice"],
  "license": "MIT",
  "authors": [{
    "name" : "Aliaksandr Harbunou",
    "email": "onekit@gmail.com",
    "homepage": "http://1kit.net"
  }],
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "^6.3.3",
    "symfony/http-foundation": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/config": "^4.0",
    "ext-openssl": "*"
  },
  "suggest": {
    "vich/uploader-bundle": "To add audio file as entity field"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Lavulator\\SpeakerRecognitionBundle\\": "src/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13",
    "liip/rmt": "^1.4"
  }
}
